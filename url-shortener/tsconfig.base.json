{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "store-prisma": ["libs/store-prisma/src/index.ts"],
      "url-shortener-core": ["libs/url-shortener-core/src/index.ts"],
      "url-shortener-service": ["libs/url-shortener-service/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
